{
   "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "baseUrl": "./",
      "paths": {
         "@/*": ["src/*"]
      },
      "lib": ["ES2015", "DOM"],
      "jsx": "preserve",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true
   }
}
